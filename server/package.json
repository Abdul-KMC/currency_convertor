{
    "name": "miniproject_part1",
    "version": "1.0.0",
    "description": "Package manager for the first Assignment in the Mini Project Roadmap",
    "main": "server.js",
    "scripts": {
        "devServer": "cross-env NODE_ENV=development nodemon server.js",
        "build:ui": "rm -rf dist && cd ../client && npm run build && cp -r dist ../server && rm -rf dist",
        "push:full": "npm run build:ui && cd ../server && git add . && git commit -m 'build' && git push",
        "testServer": "cross-env NODE_ENV=test nodemon server.js",
        "initData": "cross-env NODE_ENV=development node config/initData",
        "testInitData": "cross-env NODE_ENV=test node config/initData",
        "test": "cross-env NODE_ENV=test jest --verbose -runInBand"
    },
    "author": "Student",
    "license": "ISC",
    "dependencies": {
        "cors": "^2.8.5",
        "dotenv": "^16.4.1",
        "express": "^4.18.2",
        "morgan": "^1.10.0",
        "path": "^0.12.7",
        "pg": "^8.11.3",
        "pg-hstore": "^2.3.4",
        "sequelize": "^6.35.2"
    },
    "devDependencies": {
        "cross-env": "^7.0.3",
        "jest": "^29.7.0",
        "supertest": "^6.3.4"
    }
}